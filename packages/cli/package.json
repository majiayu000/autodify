{
  "name": "@autodify/cli",
  "version": "0.1.0",
  "description": "CLI tool for Autodify - Generate Dify workflows with natural language",
  "type": "module",
  "bin": {
    "autodify": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@autodify/core": "workspace:*",
    "commander": "^12.0.0",
    "picocolors": "^1.0.0",
    "ora": "^8.0.0",
    "ai": "^3.0.0",
    "@ai-sdk/openai": "^0.0.30",
    "@ai-sdk/anthropic": "^0.0.30"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "@types/node": "^20.10.0"
  }
}
